<MagicButton@MagicBehavior+MDRectangleFlatButton>

MDScreen:

    name: "getstarted"
    Image: 
        source: "asset/image.png"
        pos_hint: {"center_x":.5, "center_y":.65}

    MagicButton:
        id: check
        text: "Get Started"
        size_hint: .66, .065
        pos_hint: {"center_x":.5, "center_y":.18}
        font_name: "BPoppins"
        on_release:
            self.wobble()
            root.manager.transition.direction = 'left'
            root.manager.current = "signup"
